{
  "name": "sporchia/alttp_php_randmizer",
  "description": "PHP based Randomizer for ALttP",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "veetorp",
      "email": "veetor+alttp@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-bcmath": "*",
    "doctrine/dbal": "^3.4",
    "ezyang/htmlpurifier": "^4.9",
    "graham-campbell/markdown": "^14.0",
    "hashids/hashids": "^4.0",
    "laraeast/laravel-vue-i18n-generator": "^2.0",
    "laravel/framework": "^9.0",
    "laravel/horizon": "^5.0",
    "laravel/passport": "^10.0",
    "laravel/tinker": "^2.2",
    "laravel/ui": "^4.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "predis/predis": "^2.0",
    "romanzipp/laravel-twitch": "^4.4",
    "sentry/sentry": "^3.7",
    "sentry/sentry-laravel": "^2.0",
    "spatie/laravel-cookie-consent": "^3.2.2",
    "symfony/yaml": "^6.0",
    "z3/enemizer_linux": "6.1.0.180",
    "z3/enemizer_osx": "6.1.0.180",
    "z3/randomizer": "dev-v32.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "brianium/paratest": "^6.3",
    "laravel/telescope": "^4.0",
    "mockery/mockery": "^1.4.2",
    "mohammedmanssour/form-request-tester": "dev-master",
    "nunomaduro/collision": "^6.1",
    "nunomaduro/larastan": "^2.1",
    "phpunit/phpunit": "^9.3.3",
    "sempro/phpunit-pretty-print": "^1.4",
    "spatie/laravel-ignition": "^1.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "z3/randomizer",
        "version": "dev-v32.0",
        "source": {
          "url": "https://github.com/KatDevsGames/z3randomizer",
          "type": "git",
          "reference": "origin/v32.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/enemizer_linux",
        "version": "6.1.0.180",
        "dist": {
          "url": "https://github.com/tcprescott/Enimizer/releases/download/2mb-rom/ubuntu.16.04-x64.tar.gz",
          "type": "tar"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/enemizer_osx",
        "version": "6.1.0.180",
        "dist": {
          "url": "https://github.com/tcprescott/Enimizer/releases/download/2mb-rom/osx.10.12-x64.tar.gz",
          "type": "tar"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "test": [
      "phpunit --colors=always --path-coverage --coverage-clover=./report/coverage.clover --coverage-html=./report"
    ],
    "coverage": [
      "Composer\\Config::disableProcessTimeout",
      "phpdbg -qrr ./vendor/bin/phpunit --colors=always --coverage-clover=./report/php.coverage.clover --coverage-html=./coverage/php"
    ],
    "cloc": [
      "cloc --exclude-dir=node_modules,vendor,report,storage,bin,public,docs,coverage --not-match-f=\"(lock|generated)\" ."
    ]
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
